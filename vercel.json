{
  "version": 2,
  "builds": [
    {
      "src": "Flask/app.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Flask/app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.12"
  }
}
